<apex:page
  standardController="Account"
  extensions="AccountExtensionController"
  recordSetVar="accounts"
>
  <apex:form>
    <apex:pageBlock id="block">
      <apex:panelGroup>
        <apex:commandButton value="New" action="{!newRecord}" />
        <apex:commandButton value="Previous" action="{!previous}" />
        <apex:commandButton value="Next" action="{!next}" />
      </apex:panelGroup>

      <apex:pageBlockTable
        value="{!accounts}"
        var="r"
        rendered="{!NOT(ISNULL(accounts))}"
      >
        <apex:column value="{!r.Name}" />
        <apex:column value="{!r.AccountNumber}" />
      </apex:pageBlockTable>
    </apex:pageBlock>
  </apex:form>
</apex:page>
